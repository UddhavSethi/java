class Rectangle
{
    int length;
    int breadth;

    Rectangle(int a, int b)
    {
        length = a;
        breadth = b;
    }

    int area()
    {
        int result = length * breadth;
        System.out.println("length is " + length + " and breadth is " + breadth + ". Area(length*breadth) = " + result);
        return result;
    }
}

public class Main
{
    public static void main(String[] args) 
    {
        Rectangle r1 = new Rectangle(4, 5);
        r1.area();
        Rectangle r2 = new Rectangle(5, 8);
        r2.area();
    }
}
